# primitive number
Number => {
	<= Comparable
	<= Printable
	<= DebugPrintable
}

eq :!Boolean: a(Number) -> b(Number) -> return a == b
lt :!Boolean: a(Number) -> b(Number) -> return a < b
lte :!Boolean: a(Number) -> b(Number) -> return a <= b
gt :!Boolean: a(Number) -> b(Number) -> return a > b
gte :!Boolean: a(Number) -> b(Number) -> return a >= b

repr :!String: self(Number) -> {
	if self % 1 == 0 then
		return string.format("%d", self)
	end
	return string.format("%f", self)
}
