Printable = shape << <- :repr 'Self 'String

DebugPrintable = shape << <- :repr_debug 'Self 'String

From = shape {
	~ :Candidate

	<~ :from :Candidate 'Self
}

TryFrom = shape {
	~ :Candidate

	<~ :try_from :Candidate << & 'Result 'Self 'Error
}

Into = shape {
	~ :Candidate

	<~ :into 'Self :Candidate
}

TryInto = shape {
	~ :Candidate

	<~ :try_into 'Self << & 'Result :Candidate 'Error
}
