$ Printable
	repr ( Self -> Self String ) shape-word-dep
;

$ DebugPrintable
	repr_debug ( Self -> Self String ) shape-word-dep
;

$ From
	Candidate shape-generic

	from ( Candidate -> Self ) shape-word-dep
;

$ TryFrom
	Candidate shape-generic

	try_from ( Candidate -> Result< Self Error > ) shape-word-dep
;

$ Into
	Target shape-generic

	into ( Self -> Target ) shape-word-dep
;

$ TryInto
	Target shape-generic

	try_into ( Self -> Result< Target Error > ) shape-word-dep
;
